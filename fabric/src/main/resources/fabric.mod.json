{
  "schemaVersion": 1,
  "id": "cartography_peripheral",
  "version": "${version}",

  "name": "Cartography Peripheral",
  "description": "A mod that adds a peripheral to Computer Craft: Tweaked that allows interfacing with various mapping mods (Dynmap, Bluemap, Pl3xmap, and Squaremap)",
  "authors": ["Jonko0493"],
  "contact": {},

  "license": "MIT License",
  "icon": "icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.jonko0493.cartographyperipheral.fabric.CartographyPeripheralFabric"
    ]
  },
  "depends": {
    "fabricloader": ">=0.14.24",
    "minecraft": ">=1.20.1",
    "computercraft": ">=1.108.4"
  },
  "suggests": {
    "bluemap": ">=3.17",
    "dynmap": ">=3.6",
    "pl3xmap": "*",
    "squaremap": ">=1.2.0"
  }
}
